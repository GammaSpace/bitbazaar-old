  <script>
  $(document).ready(function(){
    $('[rel="popover"]').popover({
      trigger: 'hover'
    });
    
    $('body').on('submit','form.check-out,form.check-in',function(e) {
      e.preventDefault();
      $(this).hide().siblings('form').show();
      $(this).daimioSubmit();
    });
    return false;
  });
  </script>
