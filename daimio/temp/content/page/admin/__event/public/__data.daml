{permit superdo command "{profile find}" | > :profiles}
{permit superdo command "{member find by_status :paid}" | > :members}
{ifelse (
  {#event | is like :summer2015} "{:appsu2015 | > :type}"
  {#event | is like :spring2015} "{:bbapplication | > :type}"
)}

{lens get_confirmed_exhibitors type @type
  | extract "{this.depot.invite | is like :yes}"
  | extract "{:games_marketplace | is in this.my.application_type}"
  | collection sort by {* (:depot.order :ASC)}
  | > :ex_org}
