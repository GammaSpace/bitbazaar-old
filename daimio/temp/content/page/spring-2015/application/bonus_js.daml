<script>
$(document).ready(function(){

  $("input[name='depot[application_type]']").change(function() {
    if ($("#type-games-arcade").is(':checked')) {
      $("#merch").hide()
    }
    else {
      $("#merch").show()
    }
    if ($("#type-comics").is(':checked')) {
      $("#gameplay").hide()
    }
    else {
      $("#gameplay").show()
    }
  });
});
</script>



{end js}
