{lens get_confirmed_exhibitors | extract "{this.depot.invite | is like :yes}" | extract "{:games_marketplace | is in this.my.application_type}" | > :ex_org | ""}
{/@exhibitors | extract "{this.depot.invite | is like :yes}" | collection sort by {* (:depot.order :ASC)} | > :ex_org | ""}

<ul class="list-unstyled confirmed">
  {begin cat_vendors | merge with @ex_org}
    <li><p class="{depot.application_type}">{my.product} <small class="">{my.name}</small></p></li>
  {end cat_vendors}
</ul>
