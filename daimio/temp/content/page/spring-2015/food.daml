{lens get_confirmed_exhibitors | extract "{this.depot.invite | is like :yes}" | extract "{this.my.application_type | is like :food}" | > :ex_org | ""}
{/@exhibitors | extract "{this.depot.invite | is like :yes}" | collection sort by {* (:depot.order :ASC)} | > :ex_org | ""}

<ul class="list-unstyled confirmed" id="food">
  {begin cat_vendors | merge with @ex_org}
    <li><p class="{depot.application_type}">{my.name} <small class="">{my.product}</small></p></li>
  {end cat_vendors}
</ul>
